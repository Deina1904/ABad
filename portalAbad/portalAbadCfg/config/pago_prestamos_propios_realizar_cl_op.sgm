	</fJsp>
</fmtDef>

<! FORMATOS  DE ENTRADA-------------------------------------------------------------->

<fmtDef id=DatosRealizarOPHTMLFmt>
	<fHTMLReq>
		<fString dataName=ClaveOperacion>
		<!fString dataName=Correo>
	</fHTMLReq>
</fmtDef>

