<com.grupobbva.bc.col.web.prest.OPPagoPrestamosPropiosRealizarCL id=pago_prestamos_propios_realizar_cl_op operationContext=OperationContext>

	<refFormat name=jspFormat refId=RelacionConfirmacionPagoPrestOPJspFmt>
