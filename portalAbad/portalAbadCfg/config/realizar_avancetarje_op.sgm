<! BBVA  ---------------------------------------------------------------------------->
<! Operación:  PRESENTACION realizar_avancetarje_op  ---------------------------------->

<! OPERACION ------------------------------------------------------------------------>
<com.grupobbva.bc.col.web.at.OPRealizarAvanceTarje id=realizar_avancetarje_op operationContext=OperationContext>

	<refFormat name=jspFormat refId=AvanceRealizadoOPJspFmt>
	<refFormat name=jspFormat refId=ErrorOPJspFmt>
	<refFormat name=jspFormat refId=MensajeOPJspFmt>

	<refFormat name=htmlRequestFormat refId=DatosAvanceOPHTMLFmt>

	<refKColl refId=realizar_avancetarje_op-data>
	<refOM refId=identificacion_operativa_om>
	<refOM refId=retorno_avancetarje_om>
	<refOM refId=actualizar_relac_om>
	<refOM refId=bloqueo_usuario_om>

<! --------------------------------------------------------------------------------->

<! ESTADOS --------------------------------------------------------------------------->
	<navegacionOP>
		<estadoOP id=0 accion=continuar metodo=realizarAvanceTarje requestFmt=DatosAvanceOPHTMLFmt>
		<estadoOP id=1 accion=continuar metodo=realizarAvanceTarje requestFmt=DatosAvanceOPHTMLFmt>
		<estadoOP id=2 accion=continuar metodo=realizarAvanceTarje requestFmt=DatosAvanceOPHTMLFmt>
		<estadoOP id=ERROR accion=continuar metodo=realizarAvanceTarje requestFmt=DatosAvanceOPHTMLFmt>
		<estadoOP id=MENSAJE accion=continuar metodo=realizarAvanceTarje requestFmt=DatosAvanceOPHTMLFmt>
	</navegacionOP>

<! SALIDAS --------------------------------------------------------------------------->
	<resultadoOP>
		<salidaOP id=1 jspFmt=AvanceRealizadoOPJspFmt>
		<salidaOP id=2 jspFmt=AvanceRealizadoOPJspFmt>
		<salidaOP id=ERROR jspFmt=ErrorOPJspFmt>
		<salidaOP id=MENSAJE jspFmt=MensajeOPJspFmt>
	</resultadoOP>
</com.grupobbva.bc.col.web.at.OPRealizarAvanceTarje>

<! DATOS --------------------------------------------------------------------------->
<kColl id=realizar_avancetarje_op-data>
	<field id=PasswordOpe>
	<field id=Resultado>
	<field id=Mensaje>
	<field id=NumeOper>

	<!field id=RefCargo>
	<!field id=RefExtra>
	<!field id=RefMovim>
	<!field id=FechaOPe>
	<!field id=FechaValor>

	<field id=PDGroup>
	<field id=Country>
	<field id=Bank>
	<field id=MaxErrors>
	<field id=ClaveOperacion>
	<field id=Correo>
	<field id=Error>
</kColl>


<! FORMATOS --------------------------------------------------------------------------->

<fmtDef id=AvanceRealizadoOPJspFmt>
	<fJsp>
		<fString dataName=s_cod_nomconta description=usuario>
		<fString dataName=s_ip description=ip>
		<fString dataName=s_cod_nomempre description=empresa>
		<fString dataName=CtaPTarje description=ctaptarje>
		<fString dataName=NumeroTarjeta description=numtarjeta>
		<fString dataName=TipoCtaPTarje description=tipoctaptarje>
		<fNumString dataName=Importe description=importe>
        <fNumString dataName=NumeOper description=refcargoo>
        <fString dataName=ComisionPr description=comisionPr>
		<record dataName=retorno_avancetarje_om-data.Salida description=AvanceTarje>
			<fNumString dataName=REFCARGO description=refcargo>
			<fDate dataName=FECHOPER description=fechoper>
			<fDate dataName=FECHAVAL description=fechaval>
		</record>
	</fJsp>
</fmtDef>
<fmtDef id=ErrorOPJspFmt>
	<fJsp>
		<fString dataName=s_cod_nomempre description=empresa>
		<fString dataName=s_cod_nomconta description=usuario>
		<fString dataName=Error description=mensaje>
	</fJsp>
</fmtDef>

<fmtDef id=MensajeOPJspFmt>
	<fJsp>
		<fString dataName=s_cod_nomconta description=usuario>
		<fString dataName=Mensaje description=mensaje>
		<fString dataName=s_IdEstCliente description=estado_usuario>
	</fJsp>
</fmtDef>


<! FORMATOS  DE ENTRADA-------------------------------------------------------------->

<fmtDef id=DatosAvanceOPHTMLFmt>
	<fHTMLReq>
		<fString dataName=ClaveOperacion>
		<fString dataName=Correo>
	</fHTMLReq>
</fmtDef>

<! --------------------------------------------------------------------------------->

