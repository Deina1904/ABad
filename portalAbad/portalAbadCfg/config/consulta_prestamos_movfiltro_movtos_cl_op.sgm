<! OPERACION ----------------------------------------------------------------------->
<com.grupobbva.bc.col.web.prest.OPConsultaPrestamosMovfiltroMovtosCL id=consulta_prestamos_movfiltro_movtos_cl_op operationContext=OperationContext>
	<refFormat name=jspFormat refId=MovimientosOPJspFmt>
	<refFormat name=jspFormat refId=MovimientosExcelOPJspFmt>
	<refFormat name=jspFormat refId=ErrorOPJspFmt>
	<refFormat name=htmlRequestFormat refId=DatosInputOPHTMLFmt>
	<refKColl refId=consulta_prestamos_movfiltro_movtos_cl_op-data>
	<refOM refId=consulta_prestamos_movfiltro_movtos_cl_om>

<! --------------------------------------------------------------------------------->

<! ESTADOS --------------------------------------------------------------------------->
	<navegacionOP>
		<estadoOP id=ERROR accion=movimientos metodo=traerMovimientos requestFmt=DatosInputOPHTMLFmt>
		<estadoOP id=0 accion=movimientos metodo=traerMovimientos requestFmt=DatosInputOPHTMLFmt>
		<estadoOP id=1 accion=movimientos metodo=traerMovimientos requestFmt=DatosInputOPHTMLFmt>
		<estadoOP id=1 accion=movimientosExcel metodo=movimientosExcel>
		<estadoOP id=1 accion=masDatos metodo=masDatos>
		<estadoOP id=1 accion=atrasDatos metodo=atrasDatos>
	</navegacionOP>

<! SALIDAS --------------------------------------------------------------------------->
	<resultadoOP>
		<salidaOP id=1 jspFmt=MovimientosOPJspFmt>
		<salidaOP id=EXCEL jspFmt=MovimientosExcelOPJspFmt>
		<salidaOP id=TRANSGRESION jspFmt=ErrorOPJspFmt>
		<salidaOP id=ERROR jspFmt=ErrorOPJspFmt>
	</resultadoOP>
</com.grupobbva.bc.col.web.prest.OPConsultaPrestamosMovfiltroMovtosCL>

<! DATOS --------------------------------------------------------------------------->
<kColl id=consulta_prestamos_movfiltro_movtos_cl_op-data>
	<field id=AsuntoPropio>
	<field id=Error>

    <field id=Grupo1>

    <field id=Dia>
	<field id=Mes>
	<field id=Anio>

	<field id=FechaDesde>
	<field id=DiaDesde>
	<field id=MesDesde>
	<field id=AnioDesde>


	<field id=FechaHasta>
	<field id=DiaHasta>
	<field id=MesHasta>
	<field id=AnioHasta>

	<field id=DiasUlti>

	<field id=ImportDeter>
	<field id=ImportDesde>
	<field id=ImportHasta>

	<kColl id=Pagina>
		<iColl id=ListaMovimientosTotalOP>
			<kColl id=Movimiento>
			  <field id=FECHA-OPER>
			  <field id=VALOR-CAPITAL>
			  <field id=VALOR-INT-CTES>
			  <field id=VALOR-INT-MORA>
			  <field id=VALOR-GASTOS>
			  <field id=VALOR-PAGADO>
			  <field id=VALOR-SALDO>
			</kColl >
		</iColl >

		<iColl id=ListaMovimientosPagOP>
			<kColl id=Movimiento>
			  <field id=FECHA-OPER>
			  <field id=VALOR-CAPITAL>
			  <field id=VALOR-INT-CTES>
			  <field id=VALOR-INT-MORA>
			  <field id=VALOR-GASTOS>
			  <field id=VALOR-PAGADO>
			  <field id=VALOR-SALDO>
			</kColl >
		</iColl >
	</kColl>


</kColl>

