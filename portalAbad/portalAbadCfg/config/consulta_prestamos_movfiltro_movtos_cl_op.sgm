		<iCollVector dataName=Pagina.ListaMovimientosPagOP description=ListaMovtos>
			<record dataName=Movimiento>
      			<fDate dataName=FECHA-OPER><nullCheck>
				<fNumString dataName=VALOR-CAPITAL><nullCheck>
