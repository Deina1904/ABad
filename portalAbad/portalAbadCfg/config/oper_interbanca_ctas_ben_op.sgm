<! BBVA  ---------------------------------------------------------------------------->
<! Operación:  PRESENTACION oper_interbanca_ctas_ben_op  -------------------------------------------->

<! OPERACION ------------------------------------------------------------------------>
<com.grupobbva.bc.col.web.operinter.OPOperInterbancaCtasBen id=oper_interbanca_ctas_ben_op operationContext=OperationContext>
	<refFormat name=jspFormat refId=RelacionBenOPJspFmt>
	<refFormat name=jspFormat refId=ErrorOPJspFmt>
	<refFormat name=jspFormat refId=AyudaOPJspFmt>
	<refFormat name=htmlRequestFormat refId=BenefTrasOPHTMLFmt>
	<refFormat name=htmlRequestFormat refId=AyudaOPHTMLFmt>
	<refKColl refId=oper_interbanca_ctas_ben_op-data>
	<refOM refId=oper_interbanca_ctas_ben_om>


<! --------------------------------------------------------------------------------->

<! ESTADOS ------------------------------------------------------------------------->
	<navegacionOP>
		<estadoOP id=0 accion=menuBenTras metodo=ordBenef requestFmt=BenefTrasOPHTMLFmt>
		<estadoOP id=1 accion=menuBenTras metodo=ordBenef requestFmt=BenefTrasOPHTMLFmt>
		<estadoOP id=AYUDA accion=menuBenTras metodo=ordBenef >
		<estadoOP id=1 accion=ayuda metodo=ayuda  requestFmt=AyudaOPHTMLFmt>
		<estadoOP id=ERROR accion=menuBenTras metodo=ordBenef requestFmt=BenefTrasOPHTMLFmt>
		<estadoOP id=AYUDA accion=volverDeAyuda metodo=volverDeAyuda>
		<estadoOP id=1 accion=volverDeAyuda metodo=volverDeAyuda>

	</navegacionOP>

<! SALIDAS ------------------------------------------------------------------------->
	<resultadoOP>
		<salidaOP id=1 jspFmt=RelacionBenOPJspFmt>
		<salidaOP id=ERROR jspFmt=ErrorOPJspFmt>
		<salidaOP id=AYUDA jspFmt=AyudaOPJspFmt>
	</resultadoOP>
</com.grupobbva.bc.col.web.operinter.OPOperInterbancaCtasBen>

<! DATOS --------------------------------------------------------------------------->
<kColl id=oper_interbanca_ctas_ben_op-data>

	<field id=Ayuda>
	<field id=Ayuda_1 value="Seleccione la entidad financiera en donde se encuentra radicada la cuenta destino." >
	<field id=Ayuda_2 value="Seleccione el tipo de cuenta." >
	<field id=Ayuda_3 value="Digite el n&uacute;mero de cuenta. No se pueden incluir guiones, espacios, puntos, comas." >
	<field id=Ayuda_4 value="Digite el n&uacute;mero de la identificaci&oacute;n del beneficiario sin d&iacute;gitos de chequeo para el caso de NIT. No se pueden incluir guiones, espacios, puntos, comas." >
	<field id=Ayuda_5 value="Digite el nombre del beneficiario, sin incluir caracteres especiales como (&ntilde;, tildes, puntos, comas, rayas, guiones, /, par&eacute;ntesis)." >
	<field id=Ayuda_6 value="Solo se permiten valores enteros. No se pueden incluir guiones, espacios, puntos, comas." >

	<field id=Error>
	<field id=asuntoPropio>
</kColl>


<! FORMATOS ------------------------------------------------------------------------>

<fmtDef id=RelacionBenOPJspFmt>
	<fJsp>
		<fString dataName=s_cod_nomconta description=usuario>
		<fString dataName=s_cod_nomempre description=empresa>
		<fString dataName=CtaOrdPr description=ctaOrdPr>
		<fString dataName=FechaOpePr description=FechaOpePr>

		<iCollVector dataName=oper_interbanca_ctas_ben_om-data.Salida.ListaAsuntosAsociados description=ListaAsuntosAso>
			<record dataName=Asunto>
				<fString dataName=CODBANCO>
				<fString dataName=DESBANCO>
				<fString dataName=TIPOASUN>
				<fString dataName=ASUNTOAS>
				<fString dataName=NOTITUAS>
				<fString dataName=IDTITUAS>
				<fString dataName=INDVALAS>
			</record>
		</iCollVector>

		<record dataName=oper_interbanca_ctas_ben_om-data.Salida description=Saldo>
			<fNumString dataName=SALDODIS description=saldodis>
		</record>

	</fJsp>
</fmtDef>



<fmtDef id=ErrorOPJspFmt>
	<fJsp>
		<fString dataName=s_cod_nomconta description=usuario><nullCheck>
		<fString dataName=s_cod_nomempre description=empresa><nullCheck>
		<fString dataName=Error description=mensaje>
	</fJsp>
</fmtDef>

<! -------------------------------------------------------------------------------->

<fmtDef id=AyudaOPJspFmt>
	<fJsp>
		<fString dataName=s_cod_nomconta description=usuario><nullCheck>
		<fString dataName=s_cod_nomempre description=empresa><nullCheck>
		<fString dataName=Ayuda description=Ayuda><nullCheck>

		<fString dataName=Ayuda_1 description=Ayuda_1><nullCheck>
		<fString dataName=Ayuda_2 description=Ayuda_2><nullCheck>
		<fString dataName=Ayuda_3 description=Ayuda_3><nullCheck>
		<fString dataName=Ayuda_4 description=Ayuda_4><nullCheck>
		<fString dataName=Ayuda_5 description=Ayuda_5><nullCheck>
		<fString dataName=Ayuda_6 description=Ayuda_6><nullCheck>

	</fJsp>
</fmtDef>

<fmtDef id=BenefTrasOPHTMLFmt>
	<fHTMLReq>
		<fString dataName=asuntoPropio>
	</fHTMLReq>
</fmtDef>

<fmtDef id=AyudaOPHTMLFmt>
	<fHTMLReq>
		<fString dataName=Ayuda>
	</fHTMLReq>
</fmtDef>