                    <ext:label>Nombre</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="description" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Descripción de biblioteca compartida para los administradores</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Descripción</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="filesetRef" use="optional" type="pidListType"> 
            <xsd:annotation> 
                <xsd:documentation>ID de conjunto de archivos referenciados</xsd:documentation> 
